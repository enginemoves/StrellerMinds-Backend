/**
 * Service for interacting with IPFS: add, retrieve, and verify content.
 */
let IpfsService = class IpfsService {
    constructor() {
        this.ipfs = (0, ipfs_http_client_1.create)({
            url: 'https://ipfs.infura.io:5001/api/v0',
