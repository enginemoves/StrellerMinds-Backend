    async getServiceByToken(token) {
        const cacheKey = `token:${String(token)}`;
        // Check cache first
        if (this.cacheEnabled && this.serviceCache.has(cacheKey)) {
            this.logger.debug(`Service with token ${String(token)} found in cache`);
