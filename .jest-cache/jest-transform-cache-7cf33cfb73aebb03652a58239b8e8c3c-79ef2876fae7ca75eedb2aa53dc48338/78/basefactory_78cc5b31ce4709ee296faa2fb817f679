    /**
     * Create a single instance
     */
    create(options = {}) {
        const { overrides = {}, traits = [] } = options;
        let data = this.definition();
        // Apply traits
        traits.forEach(trait => {
            if (this.traits[trait]) {
                data = { ...data, ...this.traits[trait]() };
            }
        });
        // Apply overrides
        data = { ...data, ...overrides };
        return data;
    }
