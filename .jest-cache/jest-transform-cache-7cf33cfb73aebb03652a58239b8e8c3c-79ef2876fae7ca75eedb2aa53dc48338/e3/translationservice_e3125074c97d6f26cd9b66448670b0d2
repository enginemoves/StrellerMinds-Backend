                }
                else {
                    await this.createTranslation({
                        ...tmsTranslation,
                        source: translation_entity_1.TranslationSource.TMS,
                    });
                }
            }
            this.logger.log(`TMS sync completed for locale: ${locale}`);
        }
        catch (error) {
            this.logger.error(`TMS sync failed for locale: ${locale}`, error);
            throw error;
        }
    }
